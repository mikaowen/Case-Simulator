<html>
	<head>
		<!-- Load API and other and css libraries -->
		<link rel="stylesheet" type="text/css" href="css/bootstrap.min.css"/>
		<link rel="stylesheet" type="text/css" href="css/main.css"/>
	</head>
	<body>
		<div class="row" id="header" style="height:10%; margin:0; padding:0; border:0; width:100%; position:relative; z-index:100;">
			<table style="width:100%; padding:0; margin:0; height:80%;">
					<tr>
						<td id="headerInventoryButton" style="width:33%;">
							<text class="text-center">Inventory</text>
						</td>
						<td id="headerShopButton" style="width:33%;">
							<text class="text-center">Shop</text>
						</td>
						<td id="" style="width:33%;">
							<text class="text-center">Place holder text</text>
						</td>
					</tr>
			</table>
			<table id="subHeader" style="height:20%; width:100%; margin:0; padding:0;">
				<tr>
					<td style="width:100%;">
						<text class="text-center" id="moneyDisplay"></text>
					</td>
				</tr>
			</table>
		</div>


		<div class="container-fluid" id="main_container" style="border:0; margin:0; padding:0;">

			<!-- Shop container -->
			<div class="row" id="shop">
				<table>
					<tbody id="shopTable">
						<tr>
							<td><img id="shop1img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop2img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop3img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop4img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop5img" src="graphics/misc/transparent.png"/></img><text></text></td>
						</tr>
						<tr>
							<td id="shop1b" class="shopb"><button onclick="buyShopItem(1)"></button></td>
							<td id="shop2b" class="shopb"><button onclick="buyShopItem(2)"></button></td>
							<td id="shop3b" class="shopb"><button onclick="buyShopItem(3)"></button></td>
							<td id="shop4b" class="shopb"><button onclick="buyShopItem(4)"></button></td>
							<td id="shop5b" class="shopb"><button onclick="buyShopItem(5)"></button></td>
						</tr>
						<tr>
							<td><img id="shop6img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop7img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop8img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop9img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop10img" src="graphics/misc/transparent.png"/></img><text></text></td>
						</tr>
						<tr>
							<td id="shop6b" class="shopb"><button onclick="buyShopItem(6)"></button></td>
							<td id="shop7b" class="shopb"><button onclick="buyShopItem(7)"></button></td>
							<td id="shop8b" class="shopb"><button onclick="buyShopItem(8)"></button></td>
							<td id="shop9b" class="shopb"><button onclick="buyShopItem(9)"></button></td>
							<td id="shop10b" class="shopb"><button onclick="buyShopItem(10)"></button></td>
						</tr>
						<tr>
							<td><img id="shop11img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop12img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop13img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop14img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop15img" src="graphics/misc/transparent.png"/></img><text></text></td>
						</tr>
						<tr>
							<td id="shop11b" class="shopb"><button onclick="buyShopItem(11)"></button></td>
							<td id="shop12b" class="shopb"><button onclick="buyShopItem(12)"></button></td>
							<td id="shop13b" class="shopb"><button onclick="buyShopItem(13)"></button></td>
							<td id="shop14b" class="shopb"><button onclick="buyShopItem(14)"></button></td>
							<td id="shop15b" class="shopb"><button onclick="buyShopItem(15)"></button></td>
						</tr>
						<tr>
							<td><img id="shop16img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop17img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop18img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop19img" src="graphics/misc/transparent.png"/></img><text></text></td>
							<td><img id="shop20img" src="graphics/misc/transparent.png"/></img><text></text></td>
						</tr>
						<tr>
							<td id="shop16b" class="shopb"><button onclick="buyShopItem(16)"></button></td>
							<td id="shop17b" class="shopb"><button onclick="buyShopItem(17)"></button></td>
							<td id="shop18b" class="shopb"><button onclick="buyShopItem(18)"></button></td>
							<td id="shop19b" class="shopb"><button onclick="buyShopItem(19)"></button></td>
							<td id="shop20b" class="shopb"><button onclick="buyShopItem(20)"></button></td>
						</tr>
					</tbody>
				</table>
				<table class="text-center">
					<tbody>
						<tr>
							<td style="width:25%;"><button id="shopLastPage" onclick="shopLastPage()" style="margin:auto;">Last Page</button></td>
							<td style="width:50%; font-size:15px;"><text id="shopPageText"></text></td>
							<td style="width:25%;"><button id="shopNextPage" onclick="shopNextPage()" style="margin:auto;">Next Page</button></td>
						</tr>
					</tbody>
				</table>
			</div>
			<!-- Unlock case container -->
			<div class="row" id="unlockCase" style="padding:0; margin:0;">
				<table style="margin:auto; margin-top:20%; height:11em;">
					<tbody>
						<tr style="height:9em;">
							<td style="border:2px solid black; width:9em; height:9em;"><img style="width:100%; height:100%;"></img>
							<td style="width:20em; height:9em;"></td>
							<td style="border:2px solid black; width:9em; height:9em;"><img style="width:100%; height:100%;"></img></td>
						</tr>
						<tr style="height:2em;">
							<td style="border:2px solid black; width:9em; height:2em;" align="center"><text></text></td>
							<td style="width:20em; height:2em;"></td>
							<td style="border:2px solid black; width:9em; height:2em;" align="center"><text></text></td>
						</tr>
					</tbody>
				</table>
				<div style="margin:auto; margin-top:1em; width:12em; height:3em;">
					<button id="unlockCaseButton" onclick="rollCase()">Unlock container</button>
				</div>
			</div>

			<!-- Case roll container -->
			<div class="row" id="caseRollContainer" style="padding:0; margin:0;">
				<div class="col-md-4 col-xs-4" style="width:30%; height:90%; padding:0; position:relative; z-index:2"></div>
				<div class="col-md-4 col-xs-4" style="padding:0; width:40%; height:90%;">
					<div class="row" style="width:100%; height:40%; padding:0; margin:0; position:relative; z-index:2;"></div>
					<div class="row" style="width:100%; height:20%; padding:0; margin:0; position:relative;">
						<table id="caseRollDivider" style="width:100%; height:100%; position:relative; z-index:10;">
							<tbody>
								<tr>
									<td style="width:50%; border:4px solid orange;"></td>
									<td style="width:50%; border:4px solid orange;"></td>
								</tr>
							</tbody>
						</table>
						<div style="position:absolute; top:4px; left:4px; overflow:hidden;" id="caseRollDisplay">
						</div>
					</div>
					<div class="row" style="width:100%; height:40%; padding:0; margin:0; position:relative; z-index:2;"></div>
				</div>
				<div class="col-md-4 col-xs-4" style="width:30%; height:90%; padding:0; position:relative; z-index:2;"></div>
			</div>

			<!-- Inventory container -->
			<div class="row" id="inventory">
				<table>
					<tbody id="inventoryTable">
						<% for (var a=1; a<=5; a++) { %>
						<tr>
							<% for(var b=(a-1)*5+1; b<=(a-1)*5+5; b++) { %>
							<td><img id="inv<%= b %>img" src="" data-tooltip title="<%= b %>" class="onHover"/><img id="inv<%= b %>st" class="inventoryST onHover" src="graphics/misc/transparent.png" data-tooltip title="<%= b %>"></img></td>
							<% } %>
						</tr>
						<tr>
							<% for(var b=(a-1)*5+1; b<=(a-1)*5+5; b++) { %>
							<td id="inv<%= b %>bg" class="invbg"><text class="text-center"></text><br><text class="text-center"></text></td>
							<% } %>
						</tr>
						<% } %>
					</tbody>
				</table>
				<table class="text-center">
					<tbody>
						<tr>
							<td style="width:20%;"><button id="inventoryLastPage" onclick="inventoryLastPage()" style="margin:auto;">Last Page</button></td>
							<td style="width:60%; font-size:15px;"><text id="inventoryPageText"></text></td>
							<td style="width:20%;"><button id="inventoryNextPage" onclick="inventoryNextPage()" style="margin:auto;">Next Page</button></td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>

		<ul class="custom-menu onHover" id="invContextMenu">
		</ul>

		<!-- Load API and other and js libraries -->
		<script type="text/javascript" src="js/jquery-2.2.4.js"></script>
		<script type="text/javascript" src="js/bootstrap.min.js"></script>
		<script type="text/javascript" src="js/contextmenu.js"></script>
		<script type="text/javascript" src="js/tooltip.js"></script>
		<script type="text/javascript" src="js/main.js"></script>
		<script type="text/javascript" src="js/item.js"></script>
		<script type="text/javascript" src="js/inventory.js"></script>
		<script type="text/javascript" src="js/case.js"></script>
		<script type="text/javascript" src="js/itemDictionary.js"></script>
		<script type="text/javascript" src="js/shop.js"></script>


		<!-- Post initialization -->
		<script>
			postInitialize();
		</script>
	</body>
</html>
